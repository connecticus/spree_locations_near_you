<% content_for :page_title do %>
  <%= Spree.t(:venues) %>
<% end %>

<% content_for :page_actions do %>
  <%= button_link_to Spree.t(:new_venue), new_admin_venue_url, :class => "btn-success", :icon => 'add', :id => 'admin_venue_link' %>
<% end %>

<% content_for :table_filter_title do %>
  <%= Spree.t(:search) %>
<% end %>


<% if @venues.present? %>
  <%= render partial: 'venues' %>
<% else %>
  <div class="no-objects-found">
    <%=  Spree.t(:not_found_venue)  %>
  </div>
<% end %>

<%= paginate @venues %>
