<% content_for :page_title do %>
  <%= Spree.t(:editing_venue) %> '<%= @venue.name %>'
  <div class="page-actions table-cell toolbar pull-right" data-hook="toolbar">
          <%= button_link_to Spree.t(:back_venue), '/admin/venues', :class => "btn-success", :icon => 'arrow-left', :id => 'admin_venue_link' %>
  </div>
<% end %>

<%= form_for @venue, url: {controller: 'venues', action: :update, method: :post } do |f|  %>
  <div class="fieldset">
    <%= render partial: 'form', locals: {f: f} %>
  </div>
<% end %>
